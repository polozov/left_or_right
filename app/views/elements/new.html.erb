<h3>Создать новый элемент:</h3>
<%= form_for :element, url: { action: :create } do |f| %>
  <p>
    Наименование:<br />
    <%= f.text_field :name %>
  </p>
  <p>
    Изображение:<br />
    <%= f.file_field :image %>
  </p>
  <p>
    <%= f.submit 'Сохранить' %>
  </p>
<% end %><br />

<p><%= link_to 'К списку категорий', root_path %></p>
