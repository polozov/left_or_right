<h1>Все элементы категории</h1>
<% @elements.each do |e| %>
  <p><%= link_to e.name,
    category_element_path(category_id: params[:category_id], id: e.id) %> - 
  <%= link_to "Удалить",
    category_element_path(category_id: params[:category_id], id: e.id),
    method: :delete, confirm: "Вы уверены?"  %>
  </p>
<% end %>
<p><%= debug params %></p>
